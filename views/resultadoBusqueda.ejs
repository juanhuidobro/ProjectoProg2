<%-include('partials/headerDeslogueado')%>

	<%  if (searchResults.length == 0) { %> <!-- Si lo buscas y no lo encuentra aparece esto-->
		 <p class="error"> No se ha encontrado el resultado que se esta buscando </p>
	<% 	} else {  %> 
		<% for(let i = 0; i < searchResults.length; i+=1){ %>
			<li>
				<div class="col-12 col-sm-6 col-lg-3">
				<section class="product-box">
					<a href="/productos/detail/<%=searchResults[i].id%>">
				<h2 class="products-title"> <%= searchResults[i].modelo %> </h2>
			
				<figure class="product-box_image"></figure>
				<img src="<%= searchResults[i].imagen %>" alt="<%-searchResults.modelo%>" class="img">
			</a> 
				 
			</figure>
			
		</section>
		</div>
			</li>
		<% } %>
	<% } %>
	

	<ul>
		
	</ul>
	
	<!-- footer -->
	<%- include("./partials/footer") %>
	<!-- /footer -->

