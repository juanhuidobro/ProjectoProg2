<!--Completar header-->
<body>
    <!--HEADER-->
    <%- include('./header')%> 

    <div id="columna" class="container products">
        <div class="">
            <div>
                <h2 class="Products-title"> Editar Mi Perfil</h2>
            </div>

            <!-- Formulario Editar Perfil-->
            <form action="input-sign" action="/" method="GET">
                <p>
                    <label for="Username">Cambiar Nombre de Usuario:</label>
                    <input type="text" placeholder="Username" name="Nombre" id="Nombre de Usuario">
                </p>

                <p>
                    <label for="Mail">Cambiar mail:</label>
                    <input type="text" placeholder="mail@gmail.com" name="Mail" id="mail">
                </p>

                <p>
                    <label for="Password">Cambiar password:</label>
                    <input type="password" placeholder="Password" name="password" id="password">
                </p>
                <p>
                    <button id="button" type="submit" name="editar-perfil" id="editar-perfil">Guardar Cambios</button>
                </p>

                <!--Poner foto submit-->
            </form>
        </div>
    
    </div>
    

